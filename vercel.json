{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ],
  "build": "npm install && npx prisma generate",
  "buildCommand": "npm install && npx prisma generate",
  "installCommand": "npm install"
}